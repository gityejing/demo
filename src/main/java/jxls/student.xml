<?xml version="1.0" encoding="utf-8" ?>
<workbook>
    <worksheet name="Sheet1">
        <loop startRow="1" endRow="5" items="students" var="stu" varType="jxls.Student">
            
            <section startRow="1" endRow="5">
                <mapping row="1" col="0">stu.idname</mapping>
                <mapping row="1" col="1">stu.name</mapping>
                <mapping row="1" col="2">stu.subject</mapping>
                <mapping row="1" col="3">stu.scorename</mapping>
            </section>
            
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
            
        </loop>
    </worksheet>
</workbook>
